<androidx.drawerlayout.widget.DrawerLayout xmlns:android ="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dl_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <HorizontalScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <RelativeLayout android:id="@+id/content_container"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" >



            <RelativeLayout android:id="@+id/page_hospital_fragment_ek1"
                android:layout_marginTop="0dp"
                android:layout_marginStart="0dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                >

                <View   android:id="@+id/_bg__hospital_fragment_ek2"
                    android:background="@drawable/_bg__hospital_fragment_ek2_shape"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="0dp"
                    android:layout_width="410dp"
                    android:layout_height="730dp" />


                <RelativeLayout android:id="@+id/frame_16"
                    android:layout_marginTop="0dp"
                    android:layout_marginStart="0dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >

                    <View   android:id="@+id/hospital_toprectangle"
                        android:background="@drawable/hospital_toprectangle_shape"
                        android:layout_marginStart="0dp"
                        android:layout_marginTop="0dp"
                        android:layout_width="410dp"
                        android:layout_height="42dp" />

                </RelativeLayout>


                <RelativeLayout android:id="@+id/frame_15"
                    android:layout_marginTop="7dp"
                    android:layout_marginStart="367dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >

                    <ImageView android:id="@+id/menubtn"
                        android:src="@drawable/menubtn"
                        android:contentDescription="menubtn"
                        android:layout_marginStart="0dp"
                        android:layout_marginTop="0dp"
                        android:layout_width="28dp"
                        android:layout_height="28dp" />

                </RelativeLayout>


                <RelativeLayout android:id="@+id/frame_14"
                    android:layout_marginTop="10dp"
                    android:layout_marginStart="148dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >

                    <TextView android:id="@+id/hospitaltext"
                        android:fontFamily="Inter"
                        android:textSize="18sp"
                        android:textColor="@color/_bg__hospital_fragment_ek2_color"
                        android:layout_marginLeft="0dp"
                        android:layout_marginTop="0dp"
                        android:layout_width="133dp"
                        android:layout_height="35dp"
                        android:text="@string/hospitaltext_string"
                        />

                </RelativeLayout>


                <RelativeLayout android:id="@+id/frame_13"
                    android:layout_marginTop="42dp"
                    android:layout_marginStart="0dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >

                    <View   android:id="@+id/hospital_background"
                        android:background="@drawable/hospital_background_shape"
                        android:layout_marginStart="0dp"
                        android:layout_marginTop="0dp"
                        android:layout_width="410dp"
                        android:layout_height="651dp" />

                </RelativeLayout>


                <RelativeLayout android:id="@+id/frame_12"
                    android:layout_marginTop="457dp"
                    android:layout_marginStart="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >

                    <View   android:id="@+id/hospital_moreinfo_background"
                        android:background="@drawable/hospital_moreinfo_background_shape"
                        android:layout_marginStart="0dp"
                        android:layout_marginTop="0dp"
                        android:layout_width="375dp"
                        android:layout_height="215dp" />

                </RelativeLayout>


                <RelativeLayout android:id="@+id/frame_11"
                    android:layout_marginTop="591dp"
                    android:layout_marginStart="40dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >

                    <TextView android:id="@+id/hospital_roadaddress_textview"
                        android:fontFamily="Inter"
                        android:textSize="12sp"
                        android:textColor="@color/hospital_roadaddress_textview_color"
                        android:layout_marginLeft="0dp"
                        android:layout_marginTop="0dp"
                        android:layout_width="300dp"
                        android:layout_height="23dp"
                        android:text="@string/hospital_roadaddress_textview_string"
                        />

                </RelativeLayout>


                <RelativeLayout android:id="@+id/frame_10"
                    android:layout_marginTop="631dp"
                    android:layout_marginStart="40dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >

                    <TextView android:id="@+id/hospital_infopageurl_textview"
                        android:fontFamily="Inter"
                        android:textSize="12sp"
                        android:textColor="@color/hospital_infopageurl_textview_color"
                        android:layout_marginLeft="0dp"
                        android:layout_marginTop="0dp"
                        android:layout_width="350dp"
                        android:layout_height="23dp"
                        android:text="@string/hospital_infopageurl_textview_string"
                        />

                </RelativeLayout>


                <RelativeLayout android:id="@+id/frame_9"
                    android:layout_marginTop="551dp"
                    android:layout_marginStart="40dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >

                    <TextView android:id="@+id/hospital_phonenumber_textview"
                        android:fontFamily="Inter"
                        android:textSize="12sp"
                        android:textColor="@color/hospital_roadaddress_textview_color"
                        android:layout_marginLeft="0dp"
                        android:layout_marginTop="0dp"
                        android:layout_width="300dp"
                        android:layout_height="23dp"
                        android:text="@string/hospital_phonenumber_textview_string"
                        />

                </RelativeLayout>


                <RelativeLayout android:id="@+id/frame_8"
                    android:layout_marginTop="495dp"
                    android:layout_marginStart="40dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >

                    <TextView android:id="@+id/hospital_category_textview"
                        android:fontFamily="Inter"
                        android:textSize="12sp"
                        android:textColor="@color/hospital_category_textview_color"
                        android:layout_marginLeft="0dp"
                        android:layout_marginTop="0dp"
                        android:layout_width="300dp"
                        android:layout_height="23dp"
                        android:text="@string/hospital_category_textview_string"
                        />

                </RelativeLayout>


                <RelativeLayout android:id="@+id/frame_7"
                    android:layout_marginTop="468dp"
                    android:layout_marginStart="40dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >

                    <TextView android:id="@+id/hospital_place_textview"
                        android:fontFamily="Inter"
                        android:textSize="15sp"
                        android:textColor="@color/hospital_roadaddress_textview_color"
                        android:layout_marginLeft="0dp"
                        android:layout_marginTop="0dp"
                        android:layout_width="300dp"
                        android:layout_height="28.5dp"
                        android:text="@string/hospital_place_textview_string"
                        />

                </RelativeLayout>


                <RelativeLayout android:id="@+id/frame_6"
                    android:layout_marginTop="375dp"
                    android:layout_marginStart="89dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >

                    <TextView android:id="@+id/hospital_switchtextview"
                        android:fontFamily="Inter"
                        android:textSize="15sp"
                        android:textColor="@color/hospital_roadaddress_textview_color"
                        android:layout_marginLeft="0dp"
                        android:layout_marginTop="0dp"
                        android:layout_width="205.5dp"
                        android:layout_height="28.5dp"
                        android:text="@string/hospital_switchtextview_string"
                        />

                </RelativeLayout>


                <RelativeLayout android:id="@+id/frame_5"
                    android:layout_marginTop="424dp"
                    android:layout_marginStart="18dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >

                    <TextView android:id="@+id/hospital_moreinfo_textview"
                        android:fontFamily="Inter"
                        android:textSize="18sp"
                        android:textColor="@color/hospital_roadaddress_textview_color"
                        android:layout_marginLeft="0dp"
                        android:layout_marginTop="0dp"
                        android:layout_width="95dp"
                        android:layout_height="35dp"
                        android:text="@string/hospital_moreinfo_textview_string"
                        />

                </RelativeLayout>


                <RelativeLayout android:id="@+id/frame_4"
                    android:layout_marginTop="342dp"
                    android:layout_marginStart="0dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >

                    <View   android:id="@+id/hospital_middle_bar"
                        android:background="@drawable/hospital_middle_bar_shape"
                        android:layout_marginStart="0dp"
                        android:layout_marginTop="0dp"
                        android:layout_width="410dp"
                        android:layout_height="4dp" />

                </RelativeLayout>



                <com.kakao.vectormap.MapView
                    android:id="@+id/mapView"
                    android:layout_width="410dp"
                    android:layout_height="300dp"
                    android:layout_marginTop="42dp"
                    android:layout_marginStart="-4dp"
                    />



                <RelativeLayout android:id="@+id/frame_2"
                    android:layout_marginTop="530dp"
                    android:layout_marginStart="20dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >

                    <View   android:id="@+id/hospital_moreinfo_middlebar"
                        android:background="@drawable/hospital_moreinfo_middlebar_shape"
                        android:layout_marginStart="0dp"
                        android:layout_marginTop="0dp"
                        android:layout_width="375dp"
                        android:layout_height="1dp" />

                </RelativeLayout>


                <com.suke.widget.SwitchButton android:id="@+id/switchButton"
                    android:layout_marginTop="367dp"
                    android:layout_marginStart="18dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >
                </com.suke.widget.SwitchButton>

            </RelativeLayout>
            <ProgressBar
                android:id="@+id/loadingProgressBar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:visibility="gone"/>
        </RelativeLayout>

    </HorizontalScrollView>

    <include
        android:id="@+id/in_view_drawer"
        layout="@layout/view_drawer"/>
</androidx.drawerlayout.widget.DrawerLayout>