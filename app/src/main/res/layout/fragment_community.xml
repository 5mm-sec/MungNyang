<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dl_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >



    <HorizontalScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <ScrollView android:id="@+id/community_fragment"
            android:layout_width="wrap_content"
            android:layout_height="match_parent" >

            <RelativeLayout android:id="@+id/content_container"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <RelativeLayout android:id="@+id/page_community_fragment_ek1"
                    android:layout_marginTop="0dp"
                    android:layout_marginStart="0dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >

                    <View   android:id="@+id/_bg__community_fragment_ek2"
                        android:background="@drawable/_bg__community_fragment_ek2_shape"
                        android:layout_marginStart="0dp"
                        android:layout_marginTop="0dp"
                        android:layout_width="410dp"
                        android:layout_height="730dp" />


                    <RelativeLayout android:id="@+id/frame_124"
                        android:layout_marginTop="0dp"
                        android:layout_marginStart="0dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        >

                        <View   android:id="@+id/rectangle_1"
                            android:background="@drawable/savebutton_shape"
                            android:layout_marginStart="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_width="410dp"
                            android:layout_height="42dp" />

                    </RelativeLayout>

                    <RelativeLayout android:id="@+id/frame_77"
                        android:layout_marginTop="10dp"
                        android:layout_marginStart="153dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        >

                        <TextView android:id="@+id/addFriendTitle"
                            android:fontFamily="Inter"
                            android:textSize="18sp"
                            android:textColor="@color/_bg__frame_74_ek2_color"
                            android:layout_marginLeft="16dp"
                            android:layout_marginTop="0dp"
                            android:layout_width="95dp"
                            android:layout_height="35dp"
                            android:text="@string/community_fragment_textview_string"
                            />

                    </RelativeLayout>



                    <RelativeLayout android:id="@+id/frame_23"
                        android:layout_marginTop="7dp"
                        android:layout_marginStart="367dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        >


                        <ImageView android:id="@+id/community_menubtn"
                            android:src="@drawable/menu2"
                            android:contentDescription="community_menubtn"
                            android:layout_marginStart="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_width="28dp"
                            android:layout_height="28dp" />

                    </RelativeLayout>


                    <RelativeLayout android:id="@+id/frame_22"
                        android:layout_marginTop="42dp"
                        android:layout_marginStart="0dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        >

                        <View   android:id="@+id/community_mainrectangle"
                            android:background="@drawable/community_mainrectangle_shape"
                            android:layout_marginStart="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_width="410dp"
                            android:layout_height="730dp" />

                        <View
                            android:id="@+id/community_profilerectangle"
                            android:layout_width="381dp"
                            android:layout_height="136dp"
                            android:layout_marginStart="13dp"
                            android:layout_marginTop="15dp"
                            android:background="@drawable/community_profilerectangle_shape" />

                    </RelativeLayout>


                    <RelativeLayout android:id="@+id/frame_19"
                        android:layout_marginTop="210dp"
                        android:layout_marginStart="0dp"
                        android:layout_width="wrap_content"
                        android:layout_height="560dp"
                        >

                        <!-- ViewPager와 TabLayout 추가 -->
                        <androidx.viewpager.widget.ViewPager
                            android:id="@+id/viewPager"
                            android:background="@color/white"
                            android:layout_marginTop="70dp"
                            android:layout_width="match_parent"
                            android:layout_height="560dp" />

                        <com.google.android.material.tabs.TabLayout
                            android:id="@+id/tabLayout"
                            android:layout_width="match_parent"
                            android:layout_height="40dp"
                            android:layout_gravity="top"/>


                    </RelativeLayout>

                    <RelativeLayout android:id="@+id/frame_1413"
                        android:layout_marginTop="248dp"
                        android:layout_marginStart="0dp"
                        android:layout_width="wrap_content"
                        android:layout_height="32dp"
                        android:background="@color/white"
                        >

                        <View   android:id="@+id/community_line2"
                            android:background="@drawable/community_line2_shape"
                            android:layout_marginStart="0dp"
                            android:layout_marginTop="23dp"
                            android:layout_width="410dp"
                            android:layout_height="2dp" />


                        <RelativeLayout android:id="@+id/frame_15"
                            android:layout_marginTop="5dp"
                            android:layout_marginStart="43dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            >

                            <TextView
                                android:id="@+id/community_whoshow"
                                android:layout_width="40dp"
                                android:layout_height="15dp"
                                android:textSize="10dp"
                                android:layout_gravity="center_vertical|end"
                                android:layout_marginLeft="0dp"
                                android:layout_marginTop="0dp"

                                android:text="@string/community_whoshow_string"
                                android:textColor="@color/_bg__frame_25_ek1_color" />

                        </RelativeLayout>


                        <RelativeLayout android:id="@+id/frame_14"
                            android:layout_marginTop="5dp"
                            android:layout_marginStart="15dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            >

                            <TextView android:id="@+id/community_showtextview"
                                android:layout_gravity="center_vertical|end"
                                android:textColor="@color/_bg__frame_25_ek1_color"
                                android:layout_marginLeft="0dp"
                                android:layout_marginTop="0dp"
                                android:textSize="10dp"
                                android:layout_width="22dp"
                                android:layout_height="15dp"
                                android:text="@string/community_showtextview_string"
                                />



                        </RelativeLayout>

                    </RelativeLayout>

                    <RelativeLayout android:id="@+id/frame_11"
                        android:layout_marginTop="74dp"
                        android:layout_marginStart="28dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        >


                        <TextView android:id="@+id/community_nametextview"
                            android:textStyle="bold"
                            android:layout_gravity="center_vertical|end"
                            android:gravity="left"
                            android:textSize="17sp"
                            android:textColor="@color/_bg__frame_25_ek1_color"
                            android:layout_marginLeft="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_width="90dp"
                            android:layout_height="23.7dp"
                            android:text=""
                            />

                    </RelativeLayout>


                    <RelativeLayout android:id="@+id/frame_10"
                        android:layout_marginTop="103dp"
                        android:layout_marginStart="29dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        >

                        <View   android:id="@+id/_bg__frame_10_ek1"
                            android:background="@drawable/_bg__frame_10_ek1_shape"
                            android:layout_marginStart="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_width="352dp"
                            android:layout_height="2dp" />

                        <View   android:id="@+id/community_line1"
                            android:background="@drawable/community_line1_shape"
                            android:layout_marginStart="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_width="352dp"
                            android:layout_height="2dp" />

                    </RelativeLayout>


                    <RelativeLayout
                        android:id="@+id/frame_6"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="119dp"
                        android:layout_marginStart="33dp">

                        <TextView
                            android:id="@+id/community_howmanywriting"
                            android:layout_width="48dp"
                            android:layout_height="29dp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="0dp"
                            android:layout_marginTop="0dp"
                            android:text=""
                            android:textColor="@color/_bg__frame_25_ek1_color"
                            android:textSize="19sp" />

                        <TextView
                            android:id="@+id/community_howmanywriting_textview"
                            android:layout_width="50dp"
                            android:layout_height="17dp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="0dp"
                            android:layout_marginTop="29dp"
                            android:text="작성한 글"
                            android:textColor="@color/_bg__frame_25_ek1_color"
                            android:textSize="12dp" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/frame_8"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="119dp"
                        android:layout_marginStart="175dp">

                        <TextView
                            android:id="@+id/community_answer"
                            android:layout_width="59dp"
                            android:layout_height="29dp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="0dp"
                            android:layout_marginTop="0dp"
                            android:text=""
                            android:textColor="@color/_bg__frame_25_ek1_color"
                            android:textSize="19sp" />

                        <TextView
                            android:id="@+id/community_comment_textview"
                            android:layout_width="60dp"
                            android:layout_height="17dp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="0dp"
                            android:layout_marginTop="29dp"
                            android:text="작성한 댓글"
                            android:textColor="@color/_bg__frame_25_ek1_color"
                            android:textSize="12dp" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/frame_7"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="119dp"
                        android:layout_marginStart="332dp">

                        <TextView
                            android:id="@+id/community_friend"
                            android:layout_width="45dp"
                            android:layout_height="29dp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="0dp"
                            android:layout_marginTop="0dp"
                            android:text=""
                            android:textColor="@color/_bg__frame_25_ek1_color"
                            android:textSize="19sp" />

                        <TextView
                            android:id="@+id/community_friend_textview"
                            android:layout_width="23dp"
                            android:layout_height="17dp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="0dp"
                            android:layout_marginTop="29dp"
                            android:text="친구"
                            android:textColor="@color/_bg__frame_25_ek1_color"
                            android:textSize="12dp" />
                    </RelativeLayout>

                </RelativeLayout>

            </RelativeLayout>


        </ScrollView>


    </HorizontalScrollView>

    <include
    android:id="@+id/in_view_drawer"
    layout="@layout/view_drawer"/>


</androidx.drawerlayout.widget.DrawerLayout>

