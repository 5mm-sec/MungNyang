<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" >

    <RelativeLayout android:id="@+id/content_container"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <RelativeLayout android:id="@+id/page_add_schedule_ek1"
            android:layout_marginTop="0dp"
            android:layout_marginStart="0dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >

            <View   android:id="@+id/_bg__add_schedule_ek2"
                android:background="@drawable/_bg__add_schedule_ek2_shape"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp"
                android:layout_width="285dp"
                android:layout_height="372dp" />


            <RelativeLayout android:id="@+id/addschedule_frame"
                android:layout_marginTop="42dp"
                android:layout_marginStart="0dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                >


                <RelativeLayout android:id="@+id/addbutton"
                    android:layout_marginTop="300dp"
                    android:layout_marginStart="0dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >
                    <View
                        android:id="@+id/rectangle_2"
                        android:background="@drawable/rectangle_1_shape"
                        android:layout_marginStart="0dp"
                        android:layout_marginTop="0dp"
                        android:layout_width="285dp"
                        android:layout_height="42dp" />

                    <TextView android:id="@+id/addtextview"
                        android:fontFamily="Inter"
                        android:textSize="16sp"
                        android:textColor="@color/_bg__add_schedule_ek2_color"
                        android:layout_marginLeft="128dp"
                        android:layout_marginTop="5dp"
                        android:layout_width="51dp"
                        android:layout_height="30dp"
                        android:text="@string/schedule_addtextview_string"
                        />

                </RelativeLayout>

                <View
                    android:id="@+id/rectangle_20"
                    android:background="@color/white"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="0dp"
                    android:layout_width="285dp"
                    android:layout_height="300dp" />


                <RelativeLayout android:id="@+id/time_group"
                    android:layout_marginTop="239dp"
                    android:layout_marginStart="23dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >


                    <RelativeLayout android:id="@+id/af_edittext"
                        android:layout_marginTop="0dp"
                        android:layout_marginStart="110dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        >

                        <EditText
                            android:id="@+id/af_time"
                            android:background="@drawable/kgpage_edittext_shape"
                            android:layout_marginLeft="17.5dp"
                            android:layout_marginTop="4dp"
                            android:layout_width="62dp"
                            android:layout_height="30dp"
                            android:gravity="center"
                            android:inputType="time"/>

                    </RelativeLayout>

                    <TextView android:id="@+id/explan_text"
                        android:fontFamily="Inter"
                        android:textSize="20sp"
                        android:textColor="@color/af_time_color"
                        android:layout_marginLeft="86dp"
                        android:layout_marginTop="3dp"
                        android:layout_width="39dp"
                        android:layout_height="38dp"
                        android:text="@string/explan_text_string"
                        />


                    <RelativeLayout android:id="@+id/bf_edittext"
                        android:layout_marginTop="0dp"
                        android:layout_marginStart="0dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        >

                        <EditText
                            android:id="@+id/bf_time"
                            android:background="@drawable/kgpage_edittext_shape"
                            android:layout_marginLeft="17dp"
                            android:layout_marginTop="4dp"
                            android:layout_width="63dp"
                            android:layout_height="30dp"
                            android:gravity="center"
                            android:inputType="time"/>

                    </RelativeLayout>

                </RelativeLayout>

                <TextView android:id="@+id/time_textview"
                    android:fontFamily="Inter"
                    android:textSize="13sp"
                    android:textColor="@color/af_time_color"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="217dp"
                    android:layout_width="70.5dp"
                    android:layout_height="25.5dp"
                    android:text="@string/schedule_time_textview_string"
                    />


                <RelativeLayout android:id="@+id/detail_group"
                    android:layout_marginTop="61dp"
                    android:layout_marginStart="12dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >


                    <RelativeLayout android:id="@+id/detail_edittext"
                        android:layout_marginTop="22dp"
                        android:layout_marginStart="11dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        >

                        <EditText
                            android:id="@+id/detail_text"
                            android:background="@drawable/kgpage_edittext_shape"
                            android:layout_marginLeft="0dp"
                            android:layout_marginTop="7dp"
                            android:layout_width="250dp"
                            android:layout_height="120dp"
                            android:hint="일정 내용 (50자 이내)"
                            android:maxLength="50"
                            android:textSize="15dp"
                            android:inputType="textMultiLine"/>

                    </RelativeLayout>

                    <TextView android:id="@+id/detail_textview"
                        android:fontFamily="Inter"
                        android:textSize="13sp"
                        android:textColor="@color/af_time_color"
                        android:layout_marginLeft="0dp"
                        android:layout_marginTop="4dp"
                        android:layout_width="70.5dp"
                        android:layout_height="25.5dp"
                        android:text="@string/schedule_detail_textview_string"
                        />

                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/title_group"
                    android:layout_width="263dp"
                    android:layout_height="60dp"
                    android:layout_marginStart="12dp"
                    android:layout_marginTop="7dp">


                    <RelativeLayout
                        android:id="@+id/title_edittext"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="11dp"
                        android:layout_marginTop="20dp">

                        <EditText
                            android:id="@+id/title_text"
                            android:layout_width="250dp"
                            android:layout_height="30dp"
                            android:layout_marginLeft="0dp"
                            android:layout_marginTop="3.5dp"
                            android:background="@drawable/kgpage_edittext_shape"
                            android:hint="일정 제목을 작성해주세요. (20자 이내)"
                            android:inputType="textMultiLine"
                            android:maxLength="20"
                            android:textSize="15dp" />


                    </RelativeLayout>

                    <TextView
                        android:id="@+id/title_textview"
                        android:layout_width="70.5dp"
                        android:layout_height="25.5dp"
                        android:layout_marginLeft="0dp"
                        android:layout_marginTop="0dp"
                        android:fontFamily="Inter"
                        android:text="@string/schedule_title_textview_string"
                        android:textColor="@color/af_time_color"
                        android:textSize="13sp" />

                </RelativeLayout>

            </RelativeLayout>


            <RelativeLayout android:id="@+id/pagetitle"
                android:layout_marginTop="0dp"
                android:layout_marginStart="0dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                >


                <RelativeLayout android:id="@+id/menu"
                    android:layout_marginTop="7dp"
                    android:layout_marginStart="250dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    />


                <RelativeLayout android:id="@+id/pagetitletext"
                    android:layout_marginTop="10dp"
                    android:layout_marginStart="90dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >

                    <TextView android:id="@+id/schedule_tilte"
                        android:fontFamily="Inter"
                        android:textSize="18sp"
                        android:textColor="@color/black"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="0dp"
                        android:layout_width="95dp"
                        android:layout_height="35dp"
                        android:text="@string/schedule_tilte_string"
                        />

                </RelativeLayout>


                <RelativeLayout android:id="@+id/frame_130"
                    android:layout_marginTop="4dp"
                    android:layout_marginStart="5dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    />

            </RelativeLayout>

        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>