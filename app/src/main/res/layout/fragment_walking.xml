<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dl_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <RelativeLayout android:id="@+id/walking_fragment"
            android:layout_width="wrap_content"
            android:layout_height="match_parent" >

            <RelativeLayout android:id="@+id/content_container"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >



                <RelativeLayout android:id="@+id/page_walking_fragment_ek1"
                    android:layout_marginTop="0dp"
                    android:layout_marginStart="0dp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    >

                    <View   android:id="@+id/_bg__walking_fragment_ek2"
                        android:background="@drawable/_bg__walking_fragment_ek2_shape"
                        android:layout_marginStart="0dp"
                        android:layout_marginTop="0dp"
                        android:layout_width="410dp"
                        android:layout_height="730dp" />


                    <RelativeLayout android:id="@+id/frame_10"
                        android:layout_marginTop="0dp"
                        android:layout_marginStart="0dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        >

                        <View   android:id="@+id/walking_fragmenttopbar"
                            android:background="@drawable/walking_fragmenttopbar_shape"
                            android:layout_marginStart="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_width="410dp"
                            android:layout_height="42dp" />

                    </RelativeLayout>


                    <RelativeLayout android:id="@+id/frame_9"
                        android:layout_marginTop="10dp"
                        android:layout_marginStart="188dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        >

                        <TextView android:id="@+id/walking_fragmenttextview"
                            android:fontFamily="Inter"
                            android:textSize="18sp"
                            android:textColor="@color/_bg__walking_fragment_ek2_color"
                            android:layout_marginLeft="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_width="57dp"
                            android:layout_height="35dp"
                            android:text="@string/walking_fragmenttextview_string"
                            />

                    </RelativeLayout>


                    <RelativeLayout android:id="@+id/frame_8"
                        android:layout_marginTop="7dp"
                        android:layout_marginStart="367dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        >

                        <ImageView android:id="@+id/walking_menubutton"
                            android:src="@drawable/walking_menubutton"
                            android:contentDescription="walking_menubutton"
                            android:layout_marginStart="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_width="28dp"
                            android:layout_height="28dp" />

                    </RelativeLayout>


                    <RelativeLayout android:id="@+id/frame_7"
                        android:layout_marginTop="42dp"
                        android:layout_marginStart="0dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        >

                        <View   android:id="@+id/walking_main_rectangle"
                            android:background="@drawable/walking_main_rectangle_shape"
                            android:layout_marginStart="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_width="410dp"
                            android:layout_height="651dp" />



                    </RelativeLayout>


                    <RelativeLayout android:id="@+id/frame_6"
                        android:layout_marginTop="71dp"
                        android:layout_marginStart="37dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        >

                        <androidx.recyclerview.widget.RecyclerView   android:id="@+id/walking_recyclerview_area"
                            android:background="@drawable/walking_recyclerview_area_shape"
                            android:layout_marginStart="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_width="336dp"
                            android:layout_height="170dp"
                            tools:ignore="MissingClass" />

                    </RelativeLayout>


                    <RelativeLayout android:id="@+id/frame_5"
                        android:layout_marginTop="331dp"
                        android:layout_marginStart="7dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        >

                        <ImageView android:id="@+id/walking_icon"
                            android:src="@drawable/walking_icon"
                            android:contentDescription="walking_icon"
                            android:layout_marginStart="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_width="142dp"
                            android:layout_height="151dp" />

                        <Button
                            android:id="@+id/button"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Button" />

                    </RelativeLayout>


                    <RelativeLayout android:id="@+id/frame_4"
                        android:layout_marginTop="431dp"
                        android:layout_marginStart="-33dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        >


                        <RelativeLayout android:id="@+id/group_35"
                            android:layout_marginTop="0dp"
                            android:layout_marginStart="0dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            >

                            <ImageView android:id="@+id/walking_funtionrectangle"
                                android:src="@drawable/walking_funtionrectangle"
                                android:contentDescription="walking_funtionrectangle"
                                android:layout_marginStart="33dp"
                                android:layout_marginTop="75dp"
                                android:layout_width="410dp"
                                android:layout_height="187.34dp" />

                            <ImageView android:id="@+id/polygon_1"
                                android:src="@drawable/walking_polygon_1"
                                android:contentDescription="polygon_1"
                                android:layout_marginStart="40dp"
                                android:layout_marginTop="0dp"
                                android:layout_width="460dp"
                                android:layout_height="99.1dp" />


                        </RelativeLayout>

                    </RelativeLayout>


                    <RelativeLayout android:id="@+id/frame_3"
                        android:layout_marginTop="506dp"
                        android:layout_marginStart="165dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        >

                        <ImageView android:id="@+id/walking_start_imagebutton"
                            android:src="@drawable/walking_start_imagebutton"
                            android:contentDescription="walking_start_imagebutton"
                            android:layout_marginStart="0dp"
                            android:layout_marginTop="0dp"
                            android:layout_width="80dp"
                            android:layout_height="80dp" />


                    </RelativeLayout>

                    <TextView
                        android:id="@+id/petname"
                        android:layout_width="300dp"
                        android:layout_height="55dp"
                        android:layout_marginTop="600dp"
                        android:gravity="center"
                        android:layout_marginStart="50dp"
                        android:textStyle="bold"
                        android:textSize="20sp"
                        />

                    <RelativeLayout android:id="@+id/frame_1"
                        android:layout_marginTop="250dp"
                        android:layout_marginStart="170dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        >

                        <TextView android:id="@+id/walking_goal_textview"
                            android:layout_gravity="center_vertical|end"
                            android:gravity="end"
                            android:textSize="24sp"
                            android:textColor="@color/walking_goal_textview_color"
                            android:layout_marginLeft="-4dp"
                            android:layout_marginTop="0dp"
                            android:layout_width="111dp"
                            android:layout_height="46dp"
                            android:textStyle="bold"
                            android:text="@string/walking_goal_textview_string"
                            />

                    </RelativeLayout>
                    <com.suke.widget.SwitchButton android:id="@+id/switchButton"
                        android:layout_marginTop="250dp"
                        android:layout_marginStart="110dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        >
                    </com.suke.widget.SwitchButton>
                    <ImageView
                        android:id="@+id/walkingGoalTimer"
                        android:layout_marginStart="135dp"
                        android:layout_marginTop="300dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/walkingtimerdisable"
                        />
                    <ImageView
                        android:id="@+id/walkingGoalDistance"
                        android:layout_marginStart="185dp"
                        android:layout_marginTop="300dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/walkingdistancedisable"
                        />
                    <ImageView
                        android:id="@+id/walkingGoalKcal"
                        android:layout_marginStart="230dp"
                        android:layout_marginTop="300dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/walkingkcaldisable"
                        />

                </RelativeLayout>


            </RelativeLayout>

        </RelativeLayout>
    </LinearLayout>

    <include
        android:id="@+id/in_view_drawer"
        layout="@layout/view_drawer"/>
</androidx.drawerlayout.widget.DrawerLayout>